{
  "extends": [
    "airbnb",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, {"extensions": [".js"]}],
    "jsx-a11y/anchor-is-valid": "never",
    "import/extensions": "never",
    "arrow-parens": "off",
    "no-shadow": "off",
    "import/prefer-default-export": "off",
    "prettier/prettier": [
      "error", {
        "singleQuote": true,
        "trailingComma": "all"
      }
    ]
}}
